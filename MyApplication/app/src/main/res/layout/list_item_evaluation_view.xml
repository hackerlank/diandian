<?xml version="1.0" encoding="utf-8"?>
<com.dingli.diandiaan.evaluation.ListItemEvaluationView xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/center"
   >
    <LinearLayout
        android:id="@+id/lleve"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/bg_White"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        >
    <LinearLayout
        android:id="@+id/llstar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="12dp">
           <ImageView
               android:id="@+id/liangone"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:src="@mipmap/btnshistar"
               android:layout_weight="1"
               android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangtwo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangthree"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangfour"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangfive"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/anone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/antwo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/anthree"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/anfour"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/anfive"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/tvshiri"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/xiangqin"
                android:layout_marginTop="20dp"
                />
        </LinearLayout>
    </LinearLayout>
  <TextView
      android:id="@+id/tvjiapin"
      android:layout_below="@id/llstar"
      android:layout_margin="12dp"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textColor="@color/wenziyanse"
      />
    </LinearLayout>
    <View
        android:layout_below="@id/lleve"
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
</com.dingli.diandiaan.evaluation.ListItemEvaluationView>