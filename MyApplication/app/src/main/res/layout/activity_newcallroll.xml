<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_White">
    <LinearLayout
        android:id="@+id/newnavigation"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:background="@color/qianblue">
        <ImageView
            android:id="@+id/newback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/backwhites"
            android:layout_gravity="center"
            android:layout_marginLeft="15dp"
            />
        <TextView
            android:id="@+id/tvsousuo"
            android:layout_width="258dp"
            android:layout_height="26dp"
            android:background="@drawable/circle_jieshuocorner"
            android:drawableRight="@mipmap/icon_search"
            android:text="@string/inname"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:paddingRight="10dp"
            android:layout_gravity="center_vertical"
            android:textColor="@color/one_liuyan"
            android:layout_marginLeft="15dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:textColor="@color/bg_White"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:background="@color/beijinans"
        android:padding="10dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_gan"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/wanls"
            android:textColor="@color/bg_White"
            android:textStyle="bold"
            android:layout_marginLeft="5dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="如有'异常'位置偏移或超出范围,请老师核实更改"
            android:layout_marginLeft="5dp"
            android:textColor="@color/bg_White"/>
    </LinearLayout>
      <TextView
          android:id="@+id/tvyanz"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:textColor="@color/wenziyanse"
          android:layout_gravity="center_horizontal"
          android:padding="10dp"
          android:visibility="gone"
          />
        <View
            android:layout_width="match_parent"
            android:layout_height="8dp"
            android:background="@color/yiqiandao"/>
      <LinearLayout
          android:layout_width="match_parent"
          android:layout_height="50dp"
          android:paddingLeft="12dp"
          android:paddingRight="20dp"
          android:orientation="horizontal">
          <TextView
              android:id="@+id/tvquanbus"
              android:layout_width="75dp"
              android:layout_height="52dp"
              android:text="@string/quanbu"
              android:textColor="@color/qianblue"
              android:layout_gravity="center"
              android:drawableRight="@mipmap/next"
              android:gravity="center"
              android:paddingRight="10dp"
              android:paddingLeft="10dp"
              android:layout_marginTop="4dp"
              />
          <TextView
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:layout_weight="1"/>
            <TextView
                android:id="@+id/newbianji"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/bianjis"
                android:textColor="@color/qianblue"
                android:layout_gravity="right|center_vertical"/>
      </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <View
            android:layout_width="13dp"
            android:layout_height="1dp"
            android:background="@color/yiqiandao"/>
        <View
            android:id="@+id/viewqi"
            android:layout_width="75dp"
            android:layout_height="1dp"
            android:background="@color/yiqiandao"
            android:layout_marginLeft="15dp"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/yiqiandao"
        android:layout_marginLeft="88dp"
        />
    </RelativeLayout>
    <com.dingli.diandiaan.view.VerticalSwipeRefreshLayout
        android:id="@+id/refnewcall"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_White">
        <RelativeLayout
            android:id="@+id/rlzhuangtai"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_alignParentBottom="true"
            android:background="@color/forget"
            >
            <TextView
                android:id="@+id/tvcancelin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/code_click"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="@string/cancelout"
                android:layout_centerVertical="true"
                android:textColor="@color/bg_White"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:gravity="center_horizontal"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/tvcancelout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/code_click"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="@string/cancelin"
                android:layout_centerVertical="true"
                android:textColor="@color/bg_White"
                android:gravity="center_horizontal"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:visibility="gone"
                />
        </RelativeLayout>
    <com.dingli.diandiaan.view.XListView
        android:layout_above="@id/rlzhuangtai"
        android:id="@+id/xlistnew"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/bg_White"
        android:background="@color/bg_White"
        />
    </RelativeLayout>
    </com.dingli.diandiaan.view.VerticalSwipeRefreshLayout>
</LinearLayout>