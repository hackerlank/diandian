<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/itemcontact"
    >
   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="horizontal"
       android:layout_marginTop="20dp"
       >
       <ImageView
           android:id="@+id/ivcontactse"
           android:layout_width="20dp"
           android:layout_height="20dp"
           android:src="@mipmap/icon_weijie"
           android:layout_marginLeft="20dp"/>
       <TextView
           android:id="@+id/tvnamecontact"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textSize="15sp"
           android:textColor="@color/setlo"
           android:layout_marginLeft="5dp"
           android:text="2123"/>
   </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="8dp">
    <TextView
        android:id="@+id/tvriqicontact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="45dp"
        android:textSize="12sp"
        android:textColor="@color/one_liuyan"
       />
        <TextView
            android:id="@+id/tvcodecontact"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="@color/liushu"
            android:maxLines="1"
            android:ellipsize="end"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/forgetf"
        android:layout_marginLeft="20dp"/>
</LinearLayout>