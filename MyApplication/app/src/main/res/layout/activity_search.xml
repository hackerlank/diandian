<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/callrollnavigation"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:background="@color/qianblue">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="6"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10dp">
        <EditText
            android:id="@+id/contactsearch"
            android:hint="输入要查找的姓名"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/EditTextWithIcon"
            android:padding="10dp"
           android:maxLines="1"
            android:background="@drawable/circle_corner"
            android:imeOptions="actionSearch"
            android:layout_gravity="center_vertical"
            />
            <ImageView
                android:id="@+id/ivclear"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@mipmap/icon_qux"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="40dp"
                android:visibility="gone"/>
            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@mipmap/icon_searc"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"/>
        </RelativeLayout>
        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:paddingTop="13dp"
            android:paddingBottom="13dp"
            android:visibility="gone"
            />
        <TextView
            android:id="@+id/callrollsousuo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/bg_White"
            android:layout_gravity="center_vertical"
            android:padding="10dp"
            android:text="@string/quxiao"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"/>
    </LinearLayout>
    <com.dingli.diandiaan.view.VerticalSwipeRefreshLayout
        android:id="@+id/searchrefresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">
    <ListView
        android:id="@+id/lvsousuo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:listSelector="@drawable/page_list_selector"
        android:divider="@color/bg_White"
        />
    </com.dingli.diandiaan.view.VerticalSwipeRefreshLayout>
     <LinearLayout
         android:id="@+id/llbigdata"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:orientation="vertical"
         android:visibility="gone"
         >
         <LinearLayout
             android:id="@+id/llzuisearch"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginTop="20dp"
             android:gravity="center_vertical"
             >
             <ImageView
                 android:layout_width="20dp"
                 android:layout_height="20dp"
                 android:src="@mipmap/icon_weijie"
                 android:layout_marginLeft="12dp"/>
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:text="@string/zuixin"
                 android:textColor="@color/one_liuyan"/>
         </LinearLayout>
         <View
             android:layout_width="match_parent"
             android:layout_height="1dp"
             android:background="@color/forgetf"
             android:layout_marginLeft="12dp"/>
         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginTop="12dp">
         <TextView
             android:id="@+id/tvsearchone"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:textSize="14sp"
             android:background="@drawable/halfcircle_cornerfirst"
             android:paddingBottom="5dp"
             android:paddingTop="5dp"
             android:paddingRight="10dp"
             android:paddingLeft="10dp"
             android:textColor="@color/bg_White"
             android:layout_marginLeft="15dp"
             android:layout_marginBottom="14dp"
             android:maxWidth="100dp"
             android:maxLines="1"
             android:ellipsize="end"
             android:visibility="gone"
             />
             <TextView
                 android:id="@+id/tvsearchtwo"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:layout_marginBottom="14dp"
                 android:maxWidth="100dp"
                 android:maxLines="1"
                 android:ellipsize="end"
                 android:visibility="gone"/>
             <TextView
                 android:id="@+id/tvsearchthree"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:layout_marginBottom="14dp"
                 android:maxWidth="100dp"
                 android:maxLines="1"
                 android:ellipsize="end"
                 android:visibility="gone"/>
             <TextView
                 android:id="@+id/tvsearchfour"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:layout_marginBottom="14dp"
                 android:maxWidth="100dp"
                 android:maxLines="1"
                 android:ellipsize="end"
                 android:visibility="gone"/>
             <TextView
                 android:id="@+id/tvsearchfive"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:layout_marginBottom="14dp"
                 android:maxWidth="100dp"
                 android:maxLines="1"
                 android:ellipsize="end"
                 android:visibility="gone"
                 />
         </LinearLayout>
         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:orientation="horizontal">
             <TextView
                 android:id="@+id/tvsearchsix"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="15dp"
                 android:layout_marginBottom="16dp"
                 android:visibility="gone"/>
             <TextView
                 android:id="@+id/tvsearchseven"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:visibility="gone"
                 android:layout_marginBottom="16dp"/>
             <TextView
                 android:id="@+id/tvsearcheight"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:layout_marginBottom="16dp"
                 android:visibility="gone"/>
             <TextView
                 android:id="@+id/tvsearchnine"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:layout_marginBottom="16dp"
                 android:visibility="gone"/>
             <TextView
                 android:id="@+id/tvsearchten"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="14sp"
                 android:background="@drawable/halfcircle_corner"
                 android:paddingBottom="5dp"
                 android:paddingTop="5dp"
                 android:paddingRight="10dp"
                 android:paddingLeft="10dp"
                 android:textColor="@color/qingkz"
                 android:layout_marginLeft="8dp"
                 android:layout_marginBottom="16dp"
                 android:visibility="gone"/>
         </LinearLayout>
         <RelativeLayout
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:background="@color/shoutou">
             <TextView
                 android:id="@+id/tvqingk"
                 android:layout_centerHorizontal="true"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/qink"
                 android:drawableLeft="@mipmap/icon_clear"
                 android:drawablePadding="8dp"
                 android:layout_marginTop="20dp"
                 android:textSize="17dp"
                 android:textColor="@color/qingk"
                 />
         </RelativeLayout>
     </LinearLayout>
</LinearLayout>