<?xml version="1.0" encoding="utf-8"?>
<com.dingli.diandiaan.login.pingjiao.ListItemPJView xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="5dp"
    android:paddingTop="5dp"
    android:paddingBottom="5dp"
   >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <LinearLayout
        android:id="@+id/llstar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:padding="6dp">
           <ImageView
               android:id="@+id/liangone"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:src="@mipmap/btnshistar"
               android:layout_weight="1"
               android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangtwo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangthree"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangfour"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/liangfive"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnshistar"
                android:layout_weight="1"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/anone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/antwo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/anthree"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/anfour"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"/>
            <ImageView
                android:id="@+id/anfive"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@mipmap/btnxustar"
                android:layout_weight="1"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:padding="6dp"
            >
            <TextView
                android:id="@+id/tvshiri"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/xiangqin"
                />
        </LinearLayout>
    </LinearLayout>
  <TextView
      android:id="@+id/tvjiapin"
      android:layout_below="@id/llstar"
      android:layout_margin="10dp"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textColor="@color/wenziyanse"
      />
    </LinearLayout>
</com.dingli.diandiaan.login.pingjiao.ListItemPJView>